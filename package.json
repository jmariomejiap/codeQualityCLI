{
  "name": "code-quality-cli",
  "version": "0.0.1",
  "description": "Code coverage reporter - cli",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "start:dev": "npm run build && node ./src/index.js",
    "test": "npm run build && npm run test:run",
    "test:run": "cross-env NODE_ENV=test node node_modules/.bin/ava --serial",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "prepublish": "npm build",
    "cover": "nyc --reporter=html --reporter=text --reporter=json-summary npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mejiasgroup/codeQualityCLI.git"
  },
  "engines": {
    "node": "8.9.1"
  },
  "author": "Mario Mejia",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "cross-env": "5.1.3",
    "git-repo-info": "1.4.1",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "simple-git": "1.89.0"
  },
  "devDependencies": {
    "@types/node": "8.0.26",
    "ava": "0.19.1",
    "awesome-typescript-loader": "3.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-entries": "1.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "nyc": "11.3.0",
    "supertest": "3.0.0",
    "ts-loader": "2.3.4",
    "tslint": "5.7.0",
    "tslint-config-airbnb": "5.2.1",
    "typescript": "2.6.1"
  },
  "peerDependencies": {},
  "ava": {
    "files": [
      "test/test*.js"
    ],
    "source": [
      "./*.js"
    ],
    "failFast": false,
    "babel": "inherit",
    "verbose": true,
    "timeout": "60s",
    "require": []
  }
}
